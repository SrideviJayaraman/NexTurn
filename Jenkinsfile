pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('clean') {
            steps {
                echo 'cleaning now'
            }
        }
        stage('Build') {
            steps {
                echo 'Building now'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying now'
            }
        }
    }
    
}
